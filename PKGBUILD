pkgname=pacman-x-pro
pkgver=1.0
pkgrel=2
pkgdesc="An good Pacman Wrapper Tool! With much better functions! With inteligent sudo use, dep/rpm install and better AUR-Support! Over Pacaur (Search/Install/Update)!"
arch=('any')
url="http://www.archlinux.org"
license=('GPL')
install=${pkgname}.install
depends=('deb2targz' 'pacaur')
conflicts=()
source=("pacman-x-pro.tar.gz")
md5sums=('11c1a6e5199e0b8a55914ee47d1a6748')


build(){
  cp -r "$srcdir/usr" "$pkgdir/usr"  
  }
